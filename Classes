class Server
{
	private:
		int nbrOfSensors;
		bool consolActivation;
		bool logActivation;
	public:
		Server()
		{
			nbrOfSensors();
			consolActivation();
			logActivation();
		};
		Server(const Server&autre)
		{
			this->autre.nbrOfSensors=autre.nbrOfSensors;
			this->consolActivation=autre.consolActivation;
			this->logActivation=autre.logActivation;
			return *this;
		};
		~Server(){};
		Server& operator=(const Server&);
		{
			this->autre.nbrOfSensors=autre.nbrOfSensors;
			this->consolActivation=autre.consolActivation;
			this->logActivation=autre.logActivation;
			return *this;
		};
		operator<<(
}
